# Validator API Environment Variables
# Copy this file to .env and fill in your values

# Required for AI features (Get from https://makersuite.google.com/app/apikey)
GOOGLE_API_KEY=your_gemini_api_key_here

# Optional - Neo4j configuration (for future scaling)
# NEO4J_URI=bolt://localhost:7687
# NEO4J_USERNAME=neo4j
# NEO4J_PASSWORD=password

# Production - Your Vercel frontend URL (without trailing slash)
FRONTEND_URL=https://your-app.vercel.app

# Application settings
DEBUG=false
